{
  "name": "framework",
  "version": "0.0.1",
  "private": true,
  "packageManager": "yarn@3.1.1",
  "author": "shamilfrontend",
  "scripts": {
    "dev": "vite serve --mode dev",
    "build": "vite build",
    "build:dev": "yarn build --mode dev",
    "build:stage": "yarn build --mode stage",
    "build:prod": "yarn build --mode prod",
    "preview": "vite preview",
    "lint:js": "eslint '**/*.{vue,ts}'",
    "lint:js:fix": "eslint --fix '**/*.{vue,ts}'",
    "lint:style": "stylelint '**/*.{css,scss,vue}'",
    "lint:style:fix": "stylelint --fix '**/*.{css,scss,vue}'",
    "lint:types": "vue-tsc --noEmit --skipLibCheck",
    "prettier": "prettier --check --ignore-unknown '**/*'",
    "prettier:fix": "prettier --write --ignore-unknown '**/*'",
    "postinstall": "husky install",
    "test": "vitest",
    "test:coverage": "vitest --run --coverage"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.0",
    "pinia": "^2.0.13",
    "vue": "^3.2.33",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.17.10",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@qvant/prettier-config": "^0.1.0",
    "@qvant/stylelint-config": "^0.2.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-legacy": "^1.8.2",
    "@vitejs/plugin-vue": "^2.3.2",
    "@vue/compiler-sfc": "^3.2.33",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.21",
    "@vuedx/typescript-plugin-vue": "^0.7.5",
    "autoprefixer": "^10.4.7",
    "c8": "^7.11.2",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "happy-dom": "^3.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "stylelint": "^14.8.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.8",
    "vitest": "^0.10.2",
    "vue-tsc": "^0.34.11"
  }
}
